    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "dependencies": {
    "@onaio/utils": "^0.0.1",
    "@types/antd": "^1.0.0",
    "@types/yup": "^0.29.7",
    "antd": "^4.6.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "bootstrap": "^4.5.2",
    "connected-react-router": "^6.8.0",
    "fast_array_intersect": "^1.1.0",
    "flush-promises": "^1.0.2",
    "yup": "^0.29.3"
  }
